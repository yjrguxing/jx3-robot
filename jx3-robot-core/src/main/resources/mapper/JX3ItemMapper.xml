<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.jx3robot.robot.mapper.JX3ItemMapper">
    <select id="getGoodExpTime" resultType="com.jx3robot.robot.entity.JX3Item">
        select
            name,expTime
        from
            item
        where
            name = #{goodName}
    </select>
    <select id="getAllGoodExpTime" resultType="com.jx3robot.robot.entity.JX3Item">
        select
            name,expTime
        from
            item
    </select>
</mapper>
